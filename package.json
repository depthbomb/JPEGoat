{
	"name": "jpegoat_builder",
	"version": "0.0.0",
	"description": "Needs more JPEG. A desktop application to horribly compress your images.",
	"homepage": "https://bitbucket.org/depthbomb/jpegoat",
	"respository": {
		"type": "git",
		"url": "https://depthbomb@bitbucket.org/depthbomb/jpegoat.git"
	},
	"os": ["win32"],
	"cpu": ["x64","ia32"],
	"main": "./app/index.js",
	"author": "depthbomb",
	"license": "UNLICENSED",
	"scripts": {
		"start": "electron ./app",
		"test": "clear && gulp && npm run start",
		"build:def": "clear && gulp",
		"build:watch": "clear && gulp watch",
		"build:css": "clear && gulp sass",
		"pack:mac": "electron-packager ./app JPEGoat --overwrite --platform=darwin --arch=x64 --prune=true --out=./release-builds --electron-version=1.7.9",
		"pack:win": "electron-packager ./app JPEGoat --overwrite --asar --platform=win32 --arch=ia32 --prune=true --out=./release-builds --version-string.CompanyName=\"Caprine Softworks\" --version-string.FileDescription=\"Needs more JPEG. A desktop application to horribly compress your images.\" --version-string.ProductName=\"JPEGoat\" --electron-version=1.7.9",
		"pack:linux": "electron-packager ./app JPEGoat --overwrite --asar --platform=linux --arch=x64 --prune=true --out=./release-builds --electron-version=1.7.9",
		"pack:all": "clear && npm run pack:mac && npm run pack:win && npm run pack:linux && pack:post",
		"pack:post": "clear && gulp post-build"
	},
	"devDependencies": {
		"atob": "^2.0.3",
		"babel-core": "^6.26.0",
		"babel-minify": "^0.2.0",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-es2015-node5": "^1.2.0",
		"babel-preset-es2017": "^6.24.1",
		"babel-preset-stage-3": "^6.24.1",
		"btoa": "^1.1.2",
		"electron-packager": "^10.1.0",
		"eslint": "^4.12.0",
		"fs-extra": "^4.0.2",
		"gulp": "^3.9.1",
		"gulp-autoprefixer": "^4.0.0",
		"gulp-babel": "^7.0.0",
		"gulp-css-base64": "^1.3.4",
		"gulp-csso": "^3.0.0",
		"gulp-header": "^1.8.9",
		"gulp-htmlmin": "^3.0.0",
		"gulp-imagemin": "^4.0.0",
		"gulp-img64": "0.0.6",
		"gulp-intercept": "^0.1.0",
		"gulp-javascript-obfuscator": "^1.1.3",
		"gulp-replace": "^0.6.1",
		"gulp-sass": "^3.1.0",
		"md5": "^2.2.1",
		"moment": "^2.19.3"
	},
	"bugs": {
		"url": "https://bitbucket.org/depthbomb/jpegoat/issues"
	}
}
